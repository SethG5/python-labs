import time
import math
def distance(x1, x2, y1, y2):
  return math.sqrt((x2 - x1)^2 + (y2-y1)^2)

def intput(text):
  return int(input(text))

print('''
|-------------------|
 Distance Calculator
|-------------------|
''')

x1 = intput("X point A: ")
y1 = intput("Y point A: ")
x2 = intput("X point B: ")
y2 = intput("Y point B: ")

print("|-------------------|")

print(distance(x1, x2, y1, y2))
